{
  "name": "sahay-microvolunteering-app",
  "version": "1.0.0",
  "description": "Microvolunteering platform",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "frontend": "cd client && npm run dev",
    "backend": "cd Sahay-Backend && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd Sahay-Backend && npm start",
    "install-all": "npm install && cd client && npm install && cd ../Sahay-Backend && npm install",
    "render-build": "npm run install-all && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": "10.9.3"
  }
}